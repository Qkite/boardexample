{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">id</th>
        <th scope="col">title</th>
        <th scope="col">content</th>
    </tr>
    </thead>
    <tbody>

    {{#article}}

    <tr>
        {{#id}}
            <th scope="row">{{id}}</th>
        {{/id}}
        {{^id}}
            <th scope="row">unknown</th>
        {{/id}}

        {{#title}}
            <td>{{title}}</td>
        {{/title}}
        {{^title}}
            <th scope="row">unknown</th>
        {{/title}}

        {{#content}}
            <td >{{content}}</td>
        {{/content}}
        {{^content}}
            <th scope="row">unknown</th>
        {{/content}}

    </tr>
    {{/article}}


</table>
<a href = "/sample-board/write" class="btn btn-primary" role="button">글 작성하기</a>




{{>layouts/footer}}